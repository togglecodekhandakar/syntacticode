<p id="notice"><%= notice %></p>
<h1>Question Types</h1>


<div class="row">
  <div class="col-md-2">
    <%= link_to 'New Question Type', new_questiontype_path %>

  </div>

  <div class="col-md-10">

    <table class="table table-bordered">
  <thead>
    <tr>
      <th>Question Type ID</th>
      <th>Question Type Name</th>
      <th colspan="3">Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @questiontypes.each do |questiontype| %>
      <tr>
        <td><%= questiontype.questiontype_id %></td>
        <td><%= questiontype.questiontype_name %></td>
        <td><%= link_to 'Show', questiontype %></td>
        <td><%= link_to 'Edit', edit_questiontype_path(questiontype) %></td>
        <!--td><%= link_to 'Destroy', questiontype, method: :delete, data: { confirm: 'Are you sure?' } %></td-->
      </tr>
    <% end %>
  </tbody>
</table>

  </div>
</div>








