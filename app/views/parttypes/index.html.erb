<p id="notice"><%= notice %></p>

<h1>Part Types</h1>



<div class="row">
  <div class="col-md-2">
    
<%= link_to 'New Part Type', new_parttype_path %>
  </div>

  <div class="col-md-10">

    <table class="table table-bordered">
  <thead>
    <tr>
      <th>Part Type ID</th>
      <th>Part Type name</th>
      <th colspan="3">Action</th>
    </tr>
  </thead>

  <tbody>
    <% @parttypes.each do |parttype| %>
      <tr>
        <td><%= parttype.parttype_id %></td>
        <td><%= parttype.parttype_name %></td>
        <td><%= link_to 'Show', parttype %></td>
        <td><%= link_to 'Edit', edit_parttype_path(parttype) %></td>
        <!--td><%= link_to 'Destroy', parttype, method: :delete, data: { confirm: 'Are you sure?' } %></td-->
      </tr>
    <% end %>
  </tbody>
</table>

  </div>
</div>




